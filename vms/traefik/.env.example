# Example Environment Variables
DOMAIN=example.com
CF_API_EMAIL=your@email.com
CF_DNS_API_TOKEN=your-cloudflare-api-token

# Timezone
TZ=America/New_York

# User/Group IDs
PUID=1000
PGID=1000

# Paths
BACKUP_DIR=./backups
CONFIG_DIR=./config

# Traefik Settings
TRAEFIK_DASHBOARD_CREDENTIALS=your-username:your-password
ACME_EMAIL=your@email.com

# Ports
SSH_PORT=2222
HTTP_PORT=80
HTTPS_PORT=443
